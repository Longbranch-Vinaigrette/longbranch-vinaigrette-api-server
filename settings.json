{
  "devtools": {
      "name": "DevTools Server",
      "release": false,
      "repositoryName": "dev-tools-server",
      "version": "v1.0.0",
      "commands": {
        "setup": "pip3.10 install django",
        "start": "python3.10 main.py --start & echo $! > ./pid",
        "stop": "python3.10 main.py --stop && rm ./pid",
        "internalStart": "python3.10 manage.py runserver 37000"
      },
      "author": "FelixRiddle",
      "env": {
        "fileName": ".env",
        "variables": [
          {
            "formalName": "Server port",
            "varName": "PORT",
            "description": "Server port, defaults to 37000."
          }
        ]
      }
    }
  }